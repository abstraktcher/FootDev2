                       FontWeight="Bold" FontSize="30" FontFamily="Comic Sans MS" Foreground="Black" Margin="0,130,494,814" RenderTransformOrigin="0.267,1.515" Height="56" Width="157"/>
        <TextBox   VerticalAlignment="Center" HorizontalAlignment="Right"  x:Name="TxtHeading" CommandManager.PreviewExecuted="TxtCharacteristic_PreviewExecuted" PreviewTextInput="TxtCharacteristic_PreviewTextInput" PreviewKeyDown="TxtCharacteristics_PreviewKeyDown"
                       FontWeight="Bold" FontSize="30" FontFamily="Comic Sans MS" Foreground="Black" Margin="0,218,494,726" RenderTransformOrigin="0.267,1.515" Height="56" Width="157"/>
        <TextBox   VerticalAlignment="Center" HorizontalAlignment="Right"  x:Name="TxtMarking" CommandManager.PreviewExecuted="TxtCharacteristic_PreviewExecuted" PreviewTextInput="TxtCharacteristic_PreviewTextInput" PreviewKeyDown="TxtCharacteristics_PreviewKeyDown"
