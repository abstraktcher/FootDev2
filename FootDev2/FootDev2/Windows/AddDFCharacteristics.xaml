        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FootDev2.Windows"
      WindowStartupLocation="CenterScreen"
        MinHeight="1000" MinWidth="900" Icon="/Images/favicon.png" 
        ResizeMode="NoResize" WindowStyle="None"
        Title="Add Tournament| FootDev" FontSize="40" FontFamily="Comic Sans MS" Height="800" Width="500">
    <Grid>
        <TextBlock Text="Player" VerticalAlignment="Center" HorizontalAlignment="Right"
                       FontWeight="Bold" FontSize="30" FontFamily="Comic Sans MS" Foreground="Black" Margin="0,10,749,941" Width="128" Height="49"/>
        <ComboBox Margin="0,9,474,941"  VerticalAlignment="Center" HorizontalAlignment="Right" Name="CmbPlayer" IsEditable="True"
                       FontWeight="Bold" FontSize="30" FontFamily="Comic Sans MS" Foreground="Black" Width="250" Height="50"/>
        <TextBlock Text="Dribbling &#10;(1-40)" VerticalAlignment="Center" HorizontalAlignment="Right"
                       FontWeight="Bold" FontSize="30" FontFamily="Comic Sans MS" Foreground="Black" Margin="0,130,651,782" Width="226" Height="88"/>
        <TextBlock Text="Heading &#10;(1-40)" VerticalAlignment="Center" HorizontalAlignment="Right"
                       FontWeight="Bold" FontSize="30" FontFamily="Comic Sans MS" Foreground="Black" Margin="0,218,656,693" RenderTransformOrigin="0.703,4.314" Height="89" Width="221" />
        <TextBlock Text="Height" VerticalAlignment="Center" HorizontalAlignment="Right"
                       FontWeight="Bold" FontSize="30" FontFamily="Comic Sans MS" Foreground="Black" Margin="0,312,670,632" RenderTransformOrigin="0.267,1.515" Height="56" Width="207" />
        <TextBlock Text="Marking (1-40)" VerticalAlignment="Center" HorizontalAlignment="Right"
                       FontWeight="Bold" FontSize="30" FontFamily="Comic Sans MS" Foreground="Black" Margin="0,592,651,352" RenderTransformOrigin="0.267,1.515" Height="56" Width="226" />
        <TextBlock Text="Long Shots&#10;(1-40)" VerticalAlignment="Center" HorizontalAlignment="Right"
