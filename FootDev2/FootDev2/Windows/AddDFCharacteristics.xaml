                       FontWeight="Bold" FontSize="30" FontFamily="Comic Sans MS" Foreground="Black" Margin="0,674,494,270" RenderTransformOrigin="0.267,1.515" Height="56" Width="157"/>
        <TextBox   VerticalAlignment="Center" x:Name="TxtCrossing" HorizontalAlignment="Right" CommandManager.PreviewExecuted="TxtCharacteristic_PreviewExecuted" PreviewTextInput="TxtCharacteristic_PreviewTextInput" PreviewKeyDown="TxtCharacteristics_PreviewKeyDown"
                       FontWeight="Bold" FontSize="30" FontFamily="Comic Sans MS" Foreground="Black" Margin="0,130,59,814" RenderTransformOrigin="0.267,1.515" Height="56" Width="157"/>
        <TextBox   VerticalAlignment="Center" HorizontalAlignment="Right" x:Name="TxtWorkRate" CommandManager.PreviewExecuted="TxtCharacteristic_PreviewExecuted" PreviewTextInput="TxtCharacteristic_PreviewTextInput" PreviewKeyDown="TxtCharacteristics_PreviewKeyDown"
