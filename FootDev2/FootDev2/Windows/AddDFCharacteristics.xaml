        <TextBox   VerticalAlignment="Center" HorizontalAlignment="Right" x:Name="TxtBravery" CommandManager.PreviewExecuted="TxtCharacteristic_PreviewExecuted" PreviewTextInput="TxtCharacteristic_PreviewTextInput" PreviewKeyDown="TxtCharacteristics_PreviewKeyDown"
